{
  "name": "store-locator",
  "version": "1.0.0",
  "description": "Google Maps Store Locator",
  "repository": "https://github.com/knownasilya/store-locator",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production browserify index.js | uglifyjs -cm > bundle.js",
    "start": "ecstatic -p 8000 .",
    "watch": "watchify index.js -o bundle.js -dv"
  },
  "keywords": [
    "choo"
  ],
  "author": "",
  "license": "ISC",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "ecstatic": "^3.1.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.0",
    "choo": "^2.3.0",
    "uglify-js": "^2.4.24",
    "watchify": "^3.2.3"
  }
}
